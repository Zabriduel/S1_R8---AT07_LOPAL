    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="Realizar uma contagem de 50  a 0 de 5 em 5"/>
            <comment text="O contador for &#233; a melhor escolha para esse exercicio&#13;&#10;"/>
            <declare name="contadorFor, contadorWhile, contadorDo" type="Integer" array="False" size=""/>
            <output expression="&quot;Contador for&quot;" newline="True"/>
            <for variable="contadorFor" start="50" end="0" direction="dec" step="5">
                <output expression="&quot;Numero: &quot; &amp;contadorFor" newline="True"/>
            </for>
            <output expression="&quot;FIM&quot;" newline="True"/>
            <output expression="&quot;Contador While&quot;" newline="True"/>
            <assign variable="contadorWhile" expression="50"/>
            <while expression="contadorWhile &gt;= 0">
                <output expression="&quot;Numero: &quot; &amp;contadorWhile" newline="True"/>
                <assign variable="contadorWhile" expression="contadorWhile - 5"/>
            </while>
            <output expression="&quot;FIM&quot;" newline="True"/>
            <assign variable="contadorDo" expression="50"/>
            <do expression="contadorDo &gt;= 0">
                <output expression="&quot;Numero: &quot; &amp;contadorDo" newline="True"/>
                <assign variable="contadorDo" expression="contadorDo - 5"/>
            </do>
            <output expression="&quot;FIM&quot;" newline="True"/>
        </body>
    </function>
</flowgorithm>
